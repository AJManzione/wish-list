<section class="container-fluid">
    <h2 class="header-text text-center">Your Registries</h2>
    <hr class="hr">
    <ul>
        {{#each registries as |registry| }}
        <li class="card">
            <a href="/registry/{{registry.id}}">{{registry.name}}</a>
            <p>
                {{registry.user_id}}
            </p>
        </li>
        {{/each}}
    </ul>
    <button class="btn">Create New Registry</button>

</section>