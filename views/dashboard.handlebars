<section class="container-fluid">
    <ul>
        {{#each registries as |registry| }}
        <li class="card">
            <a href="/registry/{{registry.id}}">{{registry.name}}</a>
            <p>
                {{registry.user_id}}
            </p>
        </li>
        {{/each}}
    </ul>
    <button class="btn btn-primary">Create New Registry</button>

</section>